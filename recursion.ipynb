   "source": [
    "# using recursion to find factorial"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "482ca315",
   "metadata": {},
   "outputs": [],
   "source": [
    "def factorial(x):\n",
    "    if x == 0 or x == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return x * factorial(x - 1)"
   ]
  },
